<div id="wrapper">
  <div id="banner-home" class="text-center">
    <h1>Welcome Home</h1>
    <p>Rent unique places to stay from local hosts in 190+ countries.</p>
  </div>
  <div id="searchbar" class="text-center">
    <div class="container">
      <div class="row">

          <form class="form-inline">
            <div class="form-group">
              <label class="sr-only" for="city">City</label>
              <input type="text" class="form-control" id="city" placeholder="Search by city">
            </div>
            <div class="form-group">
              <label class="sr-only" for="check-in">Check-in</label>
              <input type="date" class="form-control" id="check-in" placeholder="check-in">
            </div>
            <div class="form-group">
              <label class="sr-only" for="check-out">Check-out</label>
              <input type="date" class="form-control" id="check-out" placeholder="check-out">
            </div>
            <div class="form-group">
              <label class="sr-only" for="capacity">Capacity</label>
              <input type="number" class="form-control" id="capacity" placeholder="Number of guest">
            </div>

            <button type="submit" class="btn btn-primary">Search</button>
          </form>

      </div>
    </div>
  </div>
</div>








