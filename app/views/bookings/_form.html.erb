<div id="form">
  <div>
    <div class="container">
      <div class="row">
        <div class="form-inline">

          <%= simple_form_for [@flat, @booking] do |f| %>
            <%= f.error_notification %>

            <%= f.input :start_date %>
            <%= f.input :end_date %>

            <%= f.button :submit, class: "btn btn-primary" %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
